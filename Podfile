platform :ios, '10.0'
use_frameworks!

target 'Reporters' do
    pod 'Firebase/Analytics'
    pod 'Firebase/Auth'
    pod 'Firebase/Firestore'
    pod 'Firebase/Database'
    pod 'Firebase/Messaging'
    pod 'Firebase/Storage'
    pod 'OneSignal', '>= 2.11.2', '< 3.0'
    pod 'ReachabilitySwift'
end

target 'OneSignalNotificationServiceExtension' do
  pod 'OneSignal', '>= 2.11.2', '< 3.0'
end
